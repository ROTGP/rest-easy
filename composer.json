{
    "name": "rotgp/rest-easy",
    "description": "Utilities to simplify laravel REST API requests",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ROTGP\\RestEasy\\": "src"
        },
        "classmap": [
            "src/traits"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ROTGP\\RestEasy\\Test\\": "tests/"
        },
        "classmap": [
            "tests/models",
            "tests/controllers"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^4.0.0"
    },
    "require": {
        "php": "^7.3|^7.4|^8.0",
        "fakerphp/faker": "^1.13"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ROTGP\\AuthSodium\\RestEasyServiceProvider"
            ]
        }
    }
}