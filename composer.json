{
    "name": "rotgp/rest-easy",
    "description": "Utilities to simplify laravel REST API requests",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ROTGP\\RestEasy\\": "src"
        },
        "classmap": [
            "src/traits"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "ROTGP\\RestEasy\\Test\\": "tests/"
        },
        "classmap": [
            "tests/models",
            "tests/controllers"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "orchestra/testbench": "^6.0.0"
    }
}